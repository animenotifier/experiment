{
	"compilerOptions": {
		"target": "ES6",
		"module": "ES6",
		"moduleResolution": "node",
		"strict": false,
		"strictFunctionTypes": false,
		"strictNullChecks": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"forceConsistentCasingInFileNames": true,
		"removeComments": true,
		"sourceMap": false
	}
}